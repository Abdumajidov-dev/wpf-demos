<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <UseWPF>true</UseWPF>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <AssemblyName>SyntaxHighlighting</AssemblyName>
    <RootNamespace>SyntaxHighlighting</RootNamespace>
    <DefineConstants>TRACE;NETCORE;</DefineConstants>
  </PropertyGroup>
  <ItemGroup>
    <Compile Remove="Resources\**" />
    <EmbeddedResource Remove="Resources\**" />
    <None Remove="Resources\**" />
    <Page Remove="Resources\**" />
  </ItemGroup>
  <ItemGroup>
    <Resource Include="App.ico" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" />
    <PackageReference Include="Syncfusion.Edit.WPF" Version="18.2.0.44" />
    <PackageReference Include="Syncfusion.shared.WPF" Version="18.2.0.44" />
    <PackageReference Include="Syncfusion.SfSkinManager.WPF" Version="18.2.0.44" />
    <PackageReference Include="Syncfusion.Themes.Office2016Colorful.Wpf" Version="18.2.0.44" />
    <Reference Include="System.Windows.Interactivity">
      <HintPath>..\..\..\Common\Data\Interactivity_Binaries\WPF\4.0\System.Windows.Interactivity.dll</HintPath>
    </Reference>
    <PackageReference Include="Syncfusion.Licensing" Version="18.2.0.44" />
  </ItemGroup>
</Project>