<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <UseWPF>true</UseWPF>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <AssemblyName>Localization</AssemblyName>
    <RootNamespace>Localization</RootNamespace>
    <DefineConstants>TRACE;NETCORE</DefineConstants>
  </PropertyGroup>
  <ItemGroup>
    <Resource Include="App.ico" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" />
    <PackageReference Include="Syncfusion.SfScheduler.WPF" Version="18.2.0.44" />
    <PackageReference Include="Syncfusion.Shared.WPF" Version="18.2.0.44" />
    <PackageReference Include="Syncfusion.Licensing" Version="18.2.0.44" />
    <Reference Include="Syncfusion.Themes.MaterialLight.WPF">
      <Private>True</Private>
    </Reference>
    <PackageReference Include="Syncfusion.SfSkinManager.WPF" Version="18.2.0.44" />
    <PackageReference Include="Syncfusion.SfInput.WPF" Version="18.2.0.44" />
    <PackageReference Include="Syncfusion.SfShared.WPF" Version="18.2.0.44" />
  </ItemGroup>
</Project>