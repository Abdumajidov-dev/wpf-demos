<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <UseWPF>true</UseWPF>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <AssemblyName>Invoice</AssemblyName>
    <RootNamespace>Invoice</RootNamespace>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DefineConstants>TRACE;NETCORE</DefineConstants>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DefineConstants>TRACE;NETCORE</DefineConstants>
  </PropertyGroup>
  <ItemGroup>
    <Resource Include="**\*.png" />
    <Resource Include="**\*.jpg" />
    <Resource Include="**\*.gif" />
    <None Remove="Assets\InvoiceTemplate.xlsx" />
    <None Remove="Assets\ProdutsPriceList.xml" />
    <None Remove="Assets\Template.docx" />
    <Resource Remove="Assets\SyncfusionLogo.jpg" />
    <EmbeddedResource Include="Assets\InvoiceTemplate.xlsx" />
    <EmbeddedResource Include="Assets\ProdutsPriceList.xml" />
    <EmbeddedResource Include="Assets\SyncfusionLogo.jpg" />
    <EmbeddedResource Include="Assets\Template.docx" />
    <Resource Include="App.ico" />
    <PackageReference Include="Syncfusion.Compression.Base" Version="18.2.0.44" />
    <PackageReference Include="Syncfusion.DocIO.Wpf" Version="18.2.0.44" />
    <PackageReference Include="Syncfusion.Licensing" Version="18.2.0.44" />
    <PackageReference Include="Syncfusion.OfficeChart.Base" Version="18.2.0.44" />
    <PackageReference Include="Syncfusion.Pdf.Wpf" Version="18.2.0.44" />
    <PackageReference Include="Syncfusion.Shared.WPF" Version="18.2.0.44" />
    <PackageReference Include="Syncfusion.XlsIO.Wpf" Version="18.2.0.44" />
  </ItemGroup>
</Project>