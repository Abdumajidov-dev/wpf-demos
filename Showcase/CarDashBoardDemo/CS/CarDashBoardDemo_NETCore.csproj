<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <UseWPF>true</UseWPF>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <AssemblyName>CarDashBoardDemo</AssemblyName>
    <RootNamespace>CarDashBoard</RootNamespace>
  </PropertyGroup>
  <ItemGroup>
    <Resource Include="**\*.png" />
    <None Remove="App.ico" />
    <PackageReference Include="Syncfusion.Licensing" Version="18.2.0.44" />
    <PackageReference Include="Syncfusion.SfGauge.WPF" Version="18.2.0.44" />
    <PackageReference Include="Syncfusion.Shared.WPF" Version="18.2.0.44" />
  </ItemGroup>
  <ItemGroup>
    <Content Include="App.ico">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>
</Project>